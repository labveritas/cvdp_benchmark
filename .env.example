# OpenAI API Key (preferred by OpenAI SDK)
# OPENAI_API_KEY=your_openai_api_key_here
# Legacy key used by some internal wrappers (optional)
# OPENAI_USER_KEY=your_openai_api_key_here

# Google GenAI (Vertex AI) API key for examples/agent/agent.py
# GCP_API_KEY=your_gcp_vertex_api_key_here

# Optional OpenAI-compatible base URL (e.g., proxy / alternative endpoint)
# OPENAI_BASE_URL=https://api.openai.com/v1

# Custom Model Factory path (optional)
# Uncomment and set this to use your own model factory implementation
# CUSTOM_MODEL_FACTORY=/path/to/your/custom_factory.py

# Example API Keys for other model providers
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
# NVIDIA_API_KEY=your_nvidia_api_key_here

# Benchmark settings (defaults shown in comments)
# BENCHMARK_THREADS=1
# BENCHMARK_PREFIX=work
# DEFAULT_MODEL=o4-mini

# Agent LLM model (used by examples/agent/agent.py)
# CVDP_LLM_MODEL=gemini-3-flash-preview

# Optional path to log full LLM responses (JSONL)
# CVDP_LLM_LOG_PATH=/path/to/llm_trace.jsonl

# Vertex AI (OpenAI-compatible endpoint) settings (optional)
# CVDP_VERTEX_PROJECT=your-gcp-project
# CVDP_VERTEX_LOCATION=us-central1
# CVDP_VERTEX_ENDPOINT=openapi
# CVDP_VERTEX_API_VERSION=v1
# CVDP_VERTEX_BASE_URL=https://us-central1-aiplatform.googleapis.com/v1/projects/PROJECT/locations/LOCATION/endpoints/openapi
# CVDP_VERTEX_ACCESS_TOKEN=your_vertex_access_token
# CVDP_VERTEX_QUOTA_PROJECT=your-billing-project

# Timeouts in seconds (defaults shown in comments)
# DOCKER_TIMEOUT=600
# DOCKER_TIMEOUT_AGENT=600
# MODEL_TIMEOUT=60
# TASK_TIMEOUT=300
# QUEUE_TIMEOUT=300

# Docker directory size monitoring (defaults shown in comments)
# DOCKER_QUOTA_THRESHOLD_MB=50
# DOCKER_QUOTA_CHECK_INTERVAL=1
# DOCKER_QUOTA_MIN_COMPRESS_SIZE_MB=10

# EDA Tool Infrastructure Configuration (optional)
# VERIF_EDA_IMAGE=cvdp-cadence-verif:latest
# LICENSE_NETWORK=licnetwork
# LICENSE_NETWORK_AUTO_CREATE=true
# OSS_SIM_IMAGE=ghcr.io/hdl/sim/osvb
# OSS_PNR_IMAGE=ghcr.io/hdl/impl/pnr

# Enable LLM-based subjective scoring (defaults shown in comments)
# ENABLE_SUBJECTIVE_SCORING=false 

# Git clone behavior for context-heavy datapoints
# CLONE_HTTP=1
